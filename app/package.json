    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "sigma": "^3.0.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "vis-data": "^8.0.3",
    "vis-timeline": "^8.4.0",
